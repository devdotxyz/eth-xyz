<body class="page page__profile">
<header class="profile__header">
  <div class="profile__header--hero"></div>
  <div class="container">
    <div class="profile__header--info">
      <div id="avatar-container" class="profile__avatar profile__avatar--bg">
        <img src="https://metadata.ens.domains/mainnet/avatar/{{ domainToLookup }}?v=1.0" id="profile-avatar"
       class="profile__avatar--image hide" alt="Avatar: {{ domainToLookup }}"/>
      </div>
      <div class="profile__info">
        <h1 class="profile__name">{{ domainToLookup }}</h1>
        <div id="url-container">
          <p class="profile__url">URL:
            <button class="btn btn-link profile__url--btn" data-value="https://{{ domainBeingAccessed }}" onClick="copy(this)">
              <span>https://{{ domainBeingAccessed }}</span><span
                class="btn btn-copy"><svg class="fa-icon"><use xlink:href="/static-assets/img/fa-sprite.svg#copy"></use></svg></span></button>
            <span class="notification-copied">Copied!</span></p>
             <p id="notification-container" class="hide notification__text" style="text-align: center; font-size:12px; font-style:italic; color:lightgray">
                *This profile may have offchain data that cannot be shown due to an external gateway error*
              </p>
        </div>
      </div>
    </div>
    <hr class="section__divider"/>
  </div>
</header>

<section id="loading-container">
  <div class="loading__indicator">
    <div class="loading__text">Loading</div>
  </div>
</section>
<main class="hide">
  <section class="profile__section profile__main collapse">
    <div id="profile-container" class="container">
      <button id="toggle-profile" type="button" aria-label="Expand profile" data-toggle="collapse"
              data-target=".profile__main.collapse" class="btn btn-link section__heading-1">Profile
      </button>
      <div id="profile-entry-container" class="profile__section--content">
      </div>
      <hr class="section__divider"/>
    </div>
  </section>
  <section class="profile__section profile__portfolio collapse">
    <div id="portfolio-container" class="container">
      <button id="toggle-portfolio" type="button" aria-label="Expand portfolio" data-toggle="collapse"
              data-target=".profile__portfolio.collapse" class="btn btn-link section__heading-1">Collection
      </button>
      <div class="profile__section--content">
        <div id="portfolio-entry-container">

        </div>
      </div>
      <div id="portfolio-pagination">
      </div>
      <hr class="section__divider"/>
    </div>
  </section>
  <section class="profile__section profile__addresses collapse">
    <div id="wallet-container" class="container">
      <button id="toggle-wallets" type="button" aria-label="Expand addresses" data-toggle="collapse"
              data-target=".profile__addresses.collapse" class="btn btn-link section__heading-1">Addresses
      </button>
      <div class="profile__section--content">
        <ul id="wallets-entry-container" class="profile__addresses--items list-unstyled">

          <li class="profile__addresses--item">
            <button class="profile__addresses--btn btn btn-link"
                    data-value="Ni1qbypjRiGJbBahs4OCda5MXaMK9X4aiRM9rWi8fqqzvL38k9" onClick="copy(this)">
              <span class="profile__addresses--avatar"
                    style="background-image: url('https://loremflickr.com/320/240/design?lock=12')"></span>
              <span>
                                    <span class="profile__addresses--name">Dogecoin</span>
                                    <span>
                                        <span class="profile__addresses--address"><span
                                            class="profile__addresses--address-text"></span><span
                                            class="btn btn-copy"><svg class="fa-icon"><use xlink:href="/static-assets/img/fa-sprite.svg#copy"></use></svg></span></span>
                                    </span>
                                </span>
            </button>
            <span class="notification-copied">Copied!</span>
          </li>
          <li class="profile__addresses--item">
            <button class="profile__addresses--btn btn btn-link" id="copyAddress"
                    data-value="el9CnOINPISaSvy58YCUqDPGuYNhAtLmsXU5p0iXEIRmrD5RSd" onClick="copy(this)">
              <span class="profile__addresses--avatar"
                    style="background-image: url('https://loremflickr.com/320/240/design?lock=19')"></span>
              <span>
                  <span class="profile__addresses--name">Ethereum</span>
                  <span>
                      <span class="profile__addresses--address"><span class="profile__addresses--address-text"></span><span class="btn btn-copy"><svg class="fa-icon"><use xlink:href="/static-assets/img/fa-sprite.svg#copy"></use></svg></span></span>
                  </span>
              </span>
            </button>
            <span class="notification-copied">Copied!</span>
          </li>
        </ul>
      </div>
      <hr class="section__divider"/>
    </div>
  </section>
</main>

<footer class="site-footer">
  <section class="site-footer__section--logos">
    <div class="container">
      <ul class="site-footer__logos list-unstyled">
        <li class="site-footer__logo">
          <a href="https://xyz.xyz/" target="_blank" rel="noreferrer noopener" class="site-footer__logo--link"><img src="/static-assets/img/logos/xyz-logo.svg"
               onerror="this.src='/static-assets/img/logos/xyz-logo.png';this.onerror=null;" alt="XYZ" itemprop="logo"
               class="site-footer__logo--image"></a>
        </li>
        <li class="site-footer__logo">
          <a href="https://ens.domains/" target="_blank" rel="noreferrer noopener" class="site-footer__logo--link"><img src="/static-assets/img/logos/ens-stacked.svg"
               onerror="this.src='/static-assets/img/logos/ens-stacked.png';this.onerror=null;"
               alt="Ethereum Name Service" itemprop="logo" class="site-footer__logo--image"></a>
        </li>
      </ul>
    </div>
  </section>
  <section class="site-footer__section--links">
    <div class="site-footer__questions"><a href="http://eth.xyz/">How do I get my own eth.xyz profile?</a></div>
  </section>
  <section class="site-footer__section--disclaimer">
    <div class="container">
      <hr class="section__divider"/>
      <div class="site-footer__section--disclaimer-text">
        <p>The profile content on this page is automatically generated from publicly available information provided through <a href="https://ens.domains/" target="_blank" rel="noopener noreferrer">ENS</a> and is ultimately controlled by the relevant <a href="https://ens.domains/" target="_blank" rel="noopener noreferrer">ENS</a> user. Nothing on this page implies any endorsement or affiliation between <a href="https://xyz.xyz/" target="_blank" rel="noopener noreferrer">XYZ</a> or <a href="https://ens.domains/" target="_blank" rel="noopener noreferrer">ENS</a> and the person or organization whose profile information appears on this page. This site does not use any third-party trackers.</p>
      </div>
    </div>
  </section>
  <div id="nft-modal-container" class="nft-modal__container invisible-start">
  </div>
</footer>

<script type="text/template" id="template-profile">
  <% if(description != null) { %>
  <div class="profile__bio">
    <p><%= description %></p>
  </div>
  <% } %>
  <div class="profile__social">
    <ul class="profile__social--items list-unstyled">
      <% if(url != null) { %>
      <li class="profile__social--item profile__social--url">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#link"></use>
          </svg>
        </span>
        <a href="<%= url %>" target="_blank" rel="noopener noreferrer"><%= url %></a>
      </li>
      <% } %>
      <% if(email != null) { %>
      <li class="profile__social--item profile__social--email">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#envelope"></use>
          </svg>
        </span>
        <a href="mailto:<%= email %>" target="_blank" rel="noopener noreferrer"><%= email %></a>
      </li>
      <% } %>
      <% if(github != null) { %>
      <li class="profile__social--item profile__social--github">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#github"></use>
          </svg>
        </span>
        <a href="https://github.com/<%= github %>" target="_blank" rel="noopener noreferrer"><%= github %></a>
      </li>
      <% } %>
      <% if(keybase != null) { %>
      <li class="profile__social--item profile__social--keybase">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#keybase"></use>
          </svg>
        </span>
        <a href="https://keybase.io/<%= keybase %>" target="_blank" rel="noopener noreferrer"><%= keybase %></a>
      </li>
      <% } %>
      <% if(linkedin != null) { %>
      <li class="profile__social--item profile__social--linkedin">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#linkedin"></use>
          </svg>
        </span>
        <a href="https://www.linkedin.com/in/<%= linkedin %>" target="_blank" rel="noopener noreferrer"><%= linkedin %></a>
      </li>
      <% } %>
      <% if(peepeth != null) { %>
      <li class="profile__social--item profile__social--peepeth">
        <span class="profile__social--icon"><img src="/static-assets/img/peepeth.svg" onerror="this.src='/static-assets/img/peepeth.png';this.onerror=null;" itemprop="icon" class="peepeth-icon"></span>
        <a href="https://peepeth.com/<%= peepeth %>" target="_blank" rel="noopener noreferrer"><%= peepeth %></a>
      </li>
      <% } %>
      <% if(phone != null) { %>
      <li class="profile__social--item profile__social--phone">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#phone"></use>
          </svg>
        </span>
        <a href="tel:<%= phone %>" target="_blank" rel="noopener noreferrer"><%= phone %></a>
      </li>
      <% } %>
      <% if(telegram != null) { %>
      <li class="profile__social--item profile__social--telegram">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#telegram"></use>
          </svg>
        </span>
        <a href="https://t.me/<%= telegram %>" target="_blank" rel="noopener noreferrer"><%= telegram %></a>
      </li>
      <% } %>
      <% if(twitter != null) { %>
      <li class="profile__social--item profile__social--twitter">
        <span class="profile__social--icon">
          <svg class="fa-icon">
            <use xlink:href="/static-assets/img/fa-sprite.svg#twitter"></use>
          </svg>
        </span>
        <a href="https://twitter.com/<%= twitter %>" target="_blank" rel="noopener noreferrer">@<%= twitter %></a>
      </li>
      <% } %>
      <% if(contentHash != null) { %>
      <li class="profile__social--item profile__social--url">
        <span class="profile__url--icon"><img src="/static-assets/img/hash.svg"
            onError="this.src='/static-assets/img/hash.png';this.onerror=null;"
            class="profile__url--icon-image"></span>
        <span>
          <a href="<%= contentHash %>" target="_blank" rel="noopener noreferrer"><%= contentHash %></a><% if(contentHashGateway != null) { %> <span class="text-nobreak">(<a href="https://<%= contentHashGateway %>" target="_blank" rel="noopener noreferrer"><%= contentHashGateway %></a>)</span><% } %>
        </span>
      </li>
      <% } %>
    </ul>
  </div>
</script>

<script type="text/template" id="template-nft-pagination">
  <div class="portfolio-pagination<% if(pagination.totalNumPages < 5) { %> portfolio-pagination__centered<% } %>">
  <% if(pagination.totalNumPages > 1) {%>
    <button type="button" class="btn-nft-pagination btn-nft-pagination--previous" <% if(pagination.previousPage < 1) {%>disabled<%} %> data-page="<%= pagination.previousPage %>"></button>
    <button type="button" class="btn-nft-pagination<% if(pagination.currentPage == 1) { %> current-page<% } %>"<% if(pagination.currentPage == 1) {%> disabled="disabled"<%} %> data-page="1"><span class="page-label">Page </span>1<span class="of-pages"> of <%= pagination.totalNumPages %></span></button>
    <% if(pagination.totalNumPages > 5) {%>
      <% if(pagination.currentPage > 3) {%>
    <span class="ellipsis--pagination">...</span>
      <%} %>
      <% for(i = pagination.startMiddle; i <= pagination.endMiddle; i++) {%>
    <button type="button" class="btn-nft-pagination<% if(pagination.currentPage == i) { %> current-page<% } %>"<% if(i == pagination.currentPage) {%> disabled="disabled"<%} %> data-page="<%= i %>"><span class="page-label">Page </span><%= i %><span class="of-pages"> of <%= pagination.totalNumPages %></span></button>
      <%} %>
      <% if(pagination.currentPage < pagination.totalNumPages - 2) {%>
    <span class="ellipsis--pagination">...</span>
      <%} %>
    <%} else {%>
      <% for(i = 2; i < pagination.totalNumPages; i++) {%>
    <button type="button" class="btn-nft-pagination<% if(pagination.currentPage == i) { %> current-page<% } %>"<% if(i == pagination.currentPage) {%> disabled="disabled"<%} %> data-page="<%= i %>"><span class="page-label">Page </span><%= i %><span class="of-pages"> of <%= pagination.totalNumPages %></span></button>
      <%} %>
    <%} %>

    <% if(pagination.currentPage <= pagination.totalNumPages) {%>
    <button type="button" class="btn-nft-pagination<% if(pagination.currentPage == pagination.totalNumPages) { %> current-page<% } %>"<% if(pagination.currentPage == pagination.totalNumPages) {%> disabled="disabled"<%} %> data-page="<%= pagination.totalNumPages %>"><span class="page-label">Page </span><%= pagination.totalNumPages %><span class="of-pages"> of <%= pagination.totalNumPages %></span></button>
    <%} %>
    <button type="button" class="btn-nft-pagination btn-nft-pagination--next" <% if(pagination.nextPage < 1) {%>disabled<%} %> data-page="<%= pagination.nextPage %>"></button>
  <%} %>
  </div>
</script>

<script type="text/template" id="template-portfolio-entry">
  <li class="profile__portfolio--item"<% if(image_type === 'video' || image_type === 'image' || image_type === 'nonstandard') { %> style="background-image: url('<%= image_url %>');"<% } %>>
    <button type="button" id="open-nft-modal" class="btn profile__portfolio--button" data-nft-index="<%= index %>"
          onClick="window.ethxyz.loader.renderNftModal(this)">
      <span class="profile__portfolio--item-name"><%= name %></span>
      <% if (image_type === '3d') { %>
      <div class="portfolio-image-3d__thumbnail">
        <model-viewer alt="" src="<%= image_url %>" class="portfolio-image-3d__model-viewer" auto-rotate="true" auto-play="true" camera-controls="true" ar-status="not-presenting"></model-viewer>
      </div>
      <% } %>
    </button>
  </li>
</script>

<script type="text/template" id="template-wallet-entry">
  <li class="profile__addresses--item" style="display: block">
    <button data-value="<%= address %>" class="profile__addresses--btn btn btn-link" onClick="copy(this)">
      <span class="profile__addresses--avatar" style="background-image: url('https://loremflickr.com/320/240/design?lock=9')"></span>
      <span>
        <span class="profile__addresses--name"><%= name.charAt(0).toUpperCase() + name.slice(1) %></span>
        <span class="profile__addresses--address">
          <span class="profile__addresses--address-text"><%= address %></span>
          <span class="btn btn-copy"><svg class="fa-icon"><use xlink:href="/static-assets/img/fa-sprite.svg#copy"></use></svg></span></span>
      </span>
    </button>
    <span class="notification-copied">Copied!</span>
  </li>
</script>

<script type="text/template" id="template-avatar">
  <img src="<%= avatar_url %>" onerror="this.src='<%= alt_avatar_url %>'"/>
</script>

<script type="text/template" id="template-nft-modal">
  <div class="nft-modal" id="nft-modal" role="dialog" aria-hidden="true">
    <div class="nft-modal__main">
      <div class="nft-modal__header">
        <h3 class="nft-modal__name"><%= name %></h3>
        <button type="button" class="btn btn__modal-close" onclick="window.ethxyz.loader.closeNftModal()"><svg class="fa-icon"><use xlink:href="/static-assets/img/fa-sprite.svg#times"></use></svg></button>
      </div>
      <div id="nft-modal-image-container" class="nft-modal__image-container loading">
        <% if(image_type === 'video') { %>
        <div class="nft-modal__video-container">
          <div class="video__background">
            <div class="video__foreground">
              <video controls autoplay playsinline controlslist="nodownload">
                <source src="<%= image_url %>" type="video/mp4">
                Your browser does not support the video tag.
              </video>
            </div>
          </div>
        </div>
        <% } else if (image_type === '3d') { %>
          <model-viewer alt="" src="<%= image_url %>" class="image-3d__model-viewer" auto-rotate="true" auto-play="true" camera-controls="true" ar-status="not-presenting"></model-viewer>
        <% } else { %>
          <img src="<%= image_url %>" class="nft-modal__image" />
        <% } %>
      </div>
      <div class="nft-modal__text">
        <div class="nft-modal__description">
          <h4 class="nft-modal__heading-1">Description</h4>
          <p class="nft-modal__description--text"><%= description %></p>
        </div>
        <div class="nft-modal__credits">
          <% if(creator_username != null) { %>
          <div class="nft-modal__creator">
            <% if(creator_avatar != null) { %>
            <span class="nft-modal__creator-avatar"><img src="<%= creator_avatar %>" class="nft-modal__creator-avatar--image" /></span>
            <% } %>
            <div class="nft-modal__creator-info">
              <h5 class="nft-modal__heading-2">Created By</h5>
              <div class="nft-modal__creator-username">
                <p><%= creator_username %></p>
              </div>
            </div>
          </div>
          <% } %>
          <p><a href="<%= url %>" target="_blank" rel="noopener noreferrer" class="link__external">View on OpenSea<svg class="fa-icon"><use xlink:href="/static-assets/img/fa-sprite.svg#external-link-alt"></use></svg></a></p>
        </div>
      </div>
    </div>
    <button type="button" class="btn btn__modal-go-back" onclick="window.ethxyz.loader.closeNftModal()"><svg class="fa-icon"><use xlink:href="/static-assets/img/fa-sprite.svg#chevron-left"></use></svg><span
      class="btn__modal-go-back__label">go back</span></button>
  </div>
</script>

